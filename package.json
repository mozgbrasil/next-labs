{
  "name": "@mozg/next-labs",
  "version": "0.1.0",
  "private": true,
  "cacheDirectories": [
    ".next/cache"
  ],
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "preexport": "npm run build",
    "export": "next export",
    "prestart": "npm run export",
    "server:node": "DEBUG=* node ./server",
    "server:nodemon": "DEBUG=* nodemon --exec babel-node -- ./server",
    "deploy:gh-pages": "gh-pages -d out",
    "deploy:serve": "serve -s out"
  },
  "dependencies": {
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "bootstrap": "^5.1.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "gh-pages": "^3.2.3"
  }
}
